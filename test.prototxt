name: "LeNet"
phase: "train"

operatordef {
  name: "conv1"
  type: "Convolution"
  input: "data"
  output: "conv1"
  convolution_param{
     num_output: 10
     kernel_size: 3
     stride: 2
    }
  param{
       filler{type:"xavier"}
  }
  param{
       filler{type:"xavier"}
}
}

operatordef {
  name: "bn1"
  type: "BatchNorm"
  input: "conv1"
  output: "conv1_bn"
  param{
       filler{type:"xavier"}
  }
  param{
       filler{type:"xavier"}
  }
  param{
       filler{type:"xavier"}
  }
  param{
       filler{type:"xavier"}
  }

}
operatordef {
  name: "conv2"
  type: "Convolution"
  input: "conv1_bn"
  output: "conv2"
  convolution_param{
     num_output: 20
     kernel_size: 3
     stride: 2
    }
  param{
       filler{type:"xavier"}
  }
  param{
       filler{type:"xavier"}
}

}
operatordef {
   name: "conv3"
   type: "Convolution"
   input: "conv2"
   output: "conv3"
   convolution_param{
      num_output: 25
      kernel_size: 3
      stride: 2
     }
   param{
        filler{type:"xavier"}
   }
   param{
        filler{type:"xavier"}
 }

 }